
@model IEnumerable<Category>




<div class="list-group">

    @foreach (var item in Model)
    {
        <a href="/Blog/Index/@item.CategoryId" class="list-group-item list-group-item-action @(ViewBag.SelectedCategory == item.CategoryId.ToString() ? "active" : "" )">
            @item.Name
        </a>
    }

</div>


